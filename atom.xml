---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

 <title>Vox Bloom</title>
 <link href="http://http://voxbloom.com/atom.xml" rel="self"/>
 <link href="http://voxbloom.com"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://voxbloom.com</id>
 <author>
   <name>Vox Bloom | Sunset Spark</name>
   <email>gaelen+voxbloom@sunsetspark.org</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://voxbloom.com{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://voxbloom.com/interview{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}

</feed>
